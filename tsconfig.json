{
  "compilerOptions": {
    "target": "es5",
    "module": "es6",
    "jsx": "react",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist/",
    "noImplicitAny": true,
    "allowJs": true
  },
  "include": ["src"]
}
